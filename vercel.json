{
  "version": 2,
  "builds": [
    { "src": "app/frontend/index.html", "use": "@vercel/static" },
    { "src": "app/frontend/static/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/app/frontend/static/$1" },
    { "src": "/(.*)", "dest": "/app/frontend/index.html" }
  ]
}
